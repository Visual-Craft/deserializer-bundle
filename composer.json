{
    "name": "visual-craft/deserializer-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for simplifying deserialization of API (or other) data",
    "authors": [
        {
            "name": "Visual Craft"
        }
    ],
    "autoload": {
        "psr-4": { "VisualCraft\\DeserializerBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "VisualCraft\\DeserializerBundle\\Tests\\": "tests/",
            "VisualCraft\\DeserializerBundle\\Test\\": "test/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/framework-bundle": "^5.3|^6.0",
        "symfony/serializer": "^5.3|^6.0",
        "symfony/validator": "^5.3|^6.0",
        "symfony/yaml": "^5.3|^6.0",
        "symfony/property-access": "^5.3|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
