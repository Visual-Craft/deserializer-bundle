{
    "name": "visual-craft/deserializer-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for simplifying deserialization of API (or other) data",
    "authors": [
        {
            "name": "Visual Craft"
        }
    ],
    "autoload": {
        "psr-4": { "VisualCraft\\DeserializerBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "VisualCraft\\DeserializerBundle\\Tests\\": "tests/",
            "VisualCraft\\DeserializerBundle\\Test\\": "test/"
        }
    },
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "symfony/framework-bundle": "^4.1",
        "symfony/serializer": "^3.4|^4.1",
        "symfony/validator": "^3.4|^4.1",
        "symfony/yaml": "^3.4|^4.1",
        "symfony/property-access": "^3.4|^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    }
}
